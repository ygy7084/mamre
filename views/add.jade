doctype html
html
  head
    meta(charset='utf-8')
  body
    h1
      a(href='/member') 회원관리
    ul
      each member in members
        li
          a(href='/member/'+member.phone)= member.phone
    artcle
      form(action='/member/add' method='post')
        p
          input(type='text' name='phone' placeholder='phone')
        p
          input(type='submit' value='확인')
